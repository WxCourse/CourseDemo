.mine{
    background-color:#117a46;
}
.avatar-patent{
    width:120rpx;
    height:120rpx;
    border-radius:50%;
    overflow:hidden;
}
.avatar-image{
    width:120rpx;
    height:120rpx;

}
.nickname{
    font-size:40rpx;
    color:rgb(197, 197, 197);
}
.login{
    display:flex;
    flex-direction:column;
    align-items:center;    
}
.weui-top-word{
    font-size: 50rpx;
    color: gray;
}
.fenge{
    width:100%;
    height:110rpx;
    border-bottom:1px solid rgb(204, 202, 202);
    background-color: rgb(255, 255, 255);
}
.fenge-top{
    width:100%;
    height:110rpx;
    border:1px solid rgb(204, 202, 202);
    margin-top: 35rpx;
    background-color: rgb(255, 255, 255);
}
.fenge-cell-left{
    width: 50%;
    height: 100%;
    float: left;
    display: flex;
    flex-direction:column;
    justify-content: center;
    margin-left: 8%;
}
.fenge-cell-right{
    width: 15%;
    height: 100%;
    float: right;
    display: flex;
    flex-direction:column;
    justify-content: center;
    margin-right: 3%;
}
.fenge-word{
    font-size:40rpx;
    color: rgb(80, 79, 79);
}
.fenge-icon{
    width: 60rpx;
    height: 60rpx;

}
