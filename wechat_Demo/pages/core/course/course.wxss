/* 课程表 */
.section{
  width: 100%;
  font-size: 27rpx;
  height: 30rpx;
  line-height: 27rpx;
  text-align: center;
  color: #000;
  background-color: #95f8df;
  border: ivory solid 1rpx;
}
.picker{
 color: #000;
}
.top {
  display: flex;
  flex-direction: row;
  background-color: #95f8df;
  color: #057748;
  height: 35rpx;
}

.top-text {
  width: 100rpx;
  height: 29rpx;
  font-size: 9pt;
  justify-content: center;
  display: flex;
  align-items: center;
}

.date {
  display: flex;
  flex-direction: row;
  background-color: #95f8df;
  color: #057748;
  height: 35rpx;
}
.date-text{
  width: 100rpx;
  height: 29rpx;
  font-size: 9pt;
  justify-content: flex-end;
  display: flex;
  align-items: center;
}

.flex-item {
    width: 95rpx;
    height: 100px;
    opacity:0.8;
  }
  
  .Course-item {
    position: absolute;
    justify-content: center;
    display: flex;
    align-items: center;
    border-radius: 6px;
  }
  
  .smalltext {
    font-size: 8pt;
    color: #fff;
    padding-left: 3px;
  }
  
  .scroll {
    height: 1290rpx;
    z-index: 101;
    position: fixed;
    /* background-color: #e1effd; */
  }
  image{
    display: block;
    left: 0;
    bottom: 0;
    position: absolute;
    width: 100%;
    z-index: -999;
    height: 100%;
  }
  
  .left {
    width: 40rpx;
    height: 100rpx;
    font-size: 9pt;
    justify-content: center;
    display: flex;
    align-items: center;
  }
  
  
  